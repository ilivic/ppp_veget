<Page x:Class="Veget.PageApp.PageAgroomist.PageMenuAgro"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Veget.PageApp.PageAgroomist"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageMenuAgro">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button Content="показать агрокультуры" Click="Button_Click_1"/>
            <Button Content="произвести агрокультуру" Click="Button_Click_2"/>
            <Button Content="выйти" Click="Button_Click"/>
        </StackPanel>
        <Frame NavigationUIVisibility="Hidden" Grid.Column="1" Grid.ColumnSpan="2" x:Name="MainAgroFrame" Margin="10"/>
    </Grid>
</Page>
